{
    "build": "build.sh",
    "destroy": "destroy.sh",
    "start": "docker start take_the_bill.proxy",
    "stop": "docker stop take_the_bill.proxy",
    "depends": [
        "https://raw.githubusercontent.com/hivesolutions/config/master/torus/docker/torus.json"
    ],
    "config": [{
        "name": "OMNI_DOMAIN",
        "default": "host.frontdoorhd.com",
        "persist": true
    }, {
        "name": "OMNI_HOST",
        "default": "172.17.42.1",
        "persist": true
    }, {
        "name": "OMNI_PORT",
        "default": "8080",
        "persist": true
    }, {
        "name": "TTB_DOMAIN",
        "default": "host.takethebill.com",
        "persist": true
    }, {
        "name": "TTB_HOST",
        "default": "172.17.42.1",
        "persist": true
    }, {
        "name": "TTB_PORT",
        "default": "8181",
        "persist": true
    }]
}
